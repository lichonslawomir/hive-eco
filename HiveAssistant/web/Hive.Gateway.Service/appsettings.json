{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "FileLogging": {
    "PathFormat": "logs/gateway.{Date}.log",
    "RetainedFileCountLimit": 69,
    "LogLevel": {
      "Default": "Warning",
      "Hive.Gateway.Service": "Trace",
      "BeeHive": "Trace",
      "Microsoft": "Error"
    }
  },
  "AllowedHosts": "*",
  "ComPorts": {
    "DefaultBaudRate": 921600,
    "Ports": {
      "COM3": 921600
    }
  },
  "BeeGardenConfig": {
    "HoldingKey": "self-made",
    "BeeGardenKey": "home"
  },
  "UploadMediaClient": {
    "Cloudinary": {
      "Name": "...",
      "Key": "...",
      "Secret": "..."
    }
  }
}